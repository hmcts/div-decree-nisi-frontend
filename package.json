{
  "name": "divorce-decree-nisi",
  "description": "Divorce Decree Nisi Frontend",
  "version": "0.0.2",
  "license": "MIT",
  "engines": {
    "node": ">=12.13.0"
  },
  "jest": {
    "verbose": true,
    "testURL": "https://localhost:3000"
  },
  "scripts": {
    "start": "NODE_PATH=. node server.js",
    "setup": "NODE_PATH=.",
    "dev": "NODE_PATH=. node-dev server.js",
    "dev-debug": "NODE_PATH=. node-dev --inspect=0.0.0.0:9228 server.js",
    "mocks": "NODE_PATH=. node-dev mocks/index.js",
    "test": "NODE_PATH=. NODE_ENV=testing LOG_LEVEL=OFF nyc mocha --opts 'test/unit/mocha.opts' --exit 'test/unit/**/*.test.js' --timeout 15000",
    "test:all": "yarn lint; yarn test:unit; yarn test:a11y; yarn test:validation; yarn test:functional",
    "test:unit": "NODE_PATH=. NODE_ENV=testing LOG_LEVEL=OFF mocha --opts 'test/unit/mocha.opts' --exit 'test/unit/**/*.test.js' --timeout 15000",
    "test:smoke": "NODE_PATH=. LOG_LEVEL=ERROR codeceptjs run -c test/functional/codecept.smoke.conf.js --steps --invert",
    "test:overnight": "NODE_PATH=. LOG_LEVEL=ERROR codeceptjs run-multiple saucelabs -c test/functional/codecept.overnight.conf.js --steps",
    "test:e2e": "NODE_PATH=. NODE_ENV=testing LOG_LEVEL=ERROR mocha --exit 'test/e2e/**/*.js' --timeout 15000",
    "test:coverage": "nyc check-coverage",
    "test:a11y": "NODE_PATH=. NODE_ENV=testing LOG_LEVEL=ERROR mocha --exit test/a11y --reporter spec --recursive --timeout 15000 --reporter mochawesome --reporter-options reportDir=functional-output,reportFilename=a11y",
    "sonar-scan": "node_modules/sonar-scanner/bin/sonar-scanner",
    "test:functional": "NODE_PATH=. LOG_LEVEL=ERROR codeceptjs run -c test/functional/codecept.conf.js --steps --invert --reporter mocha-multi",
    "test:fullfunctional": "NODE_PATH=. LOG_LEVEL=ERROR codeceptjs run -c test/functional/codecept.conf.js --steps --invert --reporter mocha-multi",
    "test:functional:remote": "NODE_PATH=. POINT_TO_REMOTE=true LOG_LEVEL=OFF codeceptjs run -c test/functional/codecept.conf.js --steps --invert",
    "test:crossbrowser": "./bin/run-crossbrowser-tests.sh",
    "test-crossbrowser-e2e": "NODE_PATH=. NODE_ENV=ci  LOG_LEVEL=ERROR node_modules/.bin/codeceptjs run-multiple  ${BROWSER_GROUP:-'--all'} -c test/functional/saucelabs.conf.js --grep @cross-browser-test --steps --reporter mocha-multi",
    "test:validation": "NODE_PATH=. NODE_ENV=testing LOG_LEVEL=OFF mocha --exit test/w3cjs",
    "lint": "eslint ."
  },
  "pre-commit": "lint",
  "dependencies": {
    "@hmcts/ctsc-web-chat": "^0.3.11",
    "@hmcts/div-document-express-handler": "^1.1.0",
    "@hmcts/div-idam-express-middleware": "^6.6.2",
    "@hmcts/look-and-feel": "4.0.8",
    "@hmcts/nodejs-healthcheck": "^1.7.0",
    "@hmcts/nodejs-logging": "^3.0.1",
    "@hmcts/one-per-page": "^5.3.0",
    "@hmcts/properties-volume": "^0.0.9",
    "applicationinsights": "1.6.0",
    "applicationinsights-native-metrics": "^0.0.5",
    "cmc-cookies-manager": "github:hmcts/cmc-cookies-manager#3.0.0",
    "cookie-parser": "^1.4.4",
    "cookies": "^0.8.0",
    "copy-webpack-plugin": "^4.5.2",
    "dns-packet": "^5.2.4",
    "dropzone": "^5.5.1",
    "express": "^4.17.1",
    "express-limiter": "^1.6.1",
    "formidable": "^1.2.1",
    "glob": "^7.1.2",
    "govuk-frontend": "^2.11.0",
    "handlebars": ">=4.7.7",
    "helmet": "^4.6.0",
    "helmet-csp": ">=3.4.0",
    "hpkp": "^2.0.0",
    "http-status-codes": "^1.4.0",
    "https-proxy-agent": ">=2.2.3",
    "ioredis": "^4.14.1",
    "joi": "^14.3.1",
    "jsoneditor": "^8.4.1",
    "lodash": "^4.17.21",
    "moment": "^2.24.0",
    "os": "^0.1.1",
    "paths": "^0.1.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.8",
    "serialize-javascript": ">=2.1.1",
    "superagent": "^3.8.3",
    "tmp": "^0.1.0",
    "util": "^0.12.1"
  },
  "devDependencies": {
    "@hmcts/div-idam-test-harness": "^1.6.0",
    "@hmcts/div-test-harness": "^1.0.4",
    "@hmcts/environment-variable-setter": "^1.0.0",
    "@hmcts/one-per-page-test-suite": "^5.0.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-jq": "^0.0.9",
    "codeceptjs": "^2.6.11",
    "domino": "^2.0.2",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-mocha": "^5.0.0",
    "ioredis-mock": "^4.19.0",
    "istanbul": "^0.4.5",
    "istanbul-api": "1.2.2",
    "istanbul-reports": "1.1.4",
    "jest": "^26.0.1",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.23.3",
    "mocha-multi": "^1.1.3",
    "mochawesome": "^3.0.2",
    "mockserver": "^2.1.1",
    "node-dev": "^4.0.0",
    "nyc": "12.0.1",
    "pa11y": "^4.0.1",
    "pre-commit": "^1.2.2",
    "proxyquire": "^2.0.1",
    "puppeteer": "^5.4.1",
    "randomstring": "^1.1.5",
    "require-directory": "^2.1.1",
    "rewire": "^4.0.1",
    "sinon": "^5.0.7",
    "sinon-chai": "^3.4.0",
    "sonar-scanner": "^3.1.0",
    "supertest": "^3.0.0",
    "w3cjs": "^0.4.0",
    "ws": "^7.4.6",
    "zepto-node": "^1.0.0"
  },
  "resolutions": {
    "mem": ">=4.0.0",
    "serialize-javascript": ">=2.1.1",
    "yargs-parser": ">=13.1.2 <14.0.0 || >=15.0.1 <16.0.0 || >=18.1.2",
    "bl": ">=4.0.3",
    "minimist": ">=0.2.1 <1.0.0 || >=1.2.3",
    "braces": ">= 2.3.2",
    "ini": ">=1.3.6",
    "axios": ">=0.21.1",
    "node-notifier": ">=8.0.1",
    "y18n": ">=5.0.5||>=4.0.1 <5.0.0||>=3.2.2 <4.0.0",
    "ssri": ">=6.0.2 <7.0.0 || >=8.0.1",
    "netmask": "^2.0.1",
    "diff": "^3.5.0",
    "hosted-git-info": ">=3.0.8",
    "url-parse": ">=1.5.0",
    "lodash": "^4.17.21",
    "handlebars": ">=4.7.7",
    "ua-parser-js": ">=0.7.24",
    "postcss": "^8.2.10",
    "glob-parent": "^5.1.2",
    "redis": "^2.8.0",
    "trim-newlines": "^4.0.1"
  },
  "nyc": {
    "report-dir": "coverage",
    "temp-directory": "./coverage/.nyc_output",
    "check-coverage": true,
    "per-file": true,
    "lines": 80,
    "statements": 80,
    "functions": 80,
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": true,
    "instrument": true,
    "exclude": [
      "services/redis.js",
      "steps/cookies-policy/",
      "steps/privacy-policy/",
      "steps/terms-and-conditions/",
      "test/**/*"
    ]
  }
}
